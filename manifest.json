{
    "name": "Un mot français",
    "version": "1.0",
    "description": "Un mot français",
    "manifest_version": 2,
    "chrome_url_overrides": {
    	"newtab": "home.html"
    },
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js", "content.js", "pop_content.js", "bg_colour.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "128": "icon.png"
        }
    },
    "icons": {
        "128": "icon.png"
    }
}
